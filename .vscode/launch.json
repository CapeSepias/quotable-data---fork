  "version": "0.2.0",
  "configurations": [
    {
      // For debugging node js files
      // This will run the current file in debug mode
      "type": "node",
      "request": "launch",
      "name": "Node ${file}",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "runtimeExecutable": "node",
    }
  ]
}
